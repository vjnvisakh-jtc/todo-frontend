{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["components", "pages"],
    "includePattern": "\\.(jsx|js|ts|tsx)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["node_modules/better-docs/typescript"],
  "typescript": {
    "moduleRoot": "test"
  },
  "templates": {
    "better-docs": {
      "name": "My React components"
    }
  },
  "opts": {
    "template": "node_modules/better-docs",
    "destination": "docs",
    "recurse": true,
    "readme": "README.md"
  }
}
